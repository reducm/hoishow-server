<table class="table table-hover messages">
  <thead>
    <tr>
      <th>消息类别</th>
      <th>推送用户</th>
      <th>通知栏内容</th>
      <th>消息标题</th>
      <th>消息内容</th>
      <th>推送发送数</th>
      <th>推送查看数</th>
      <th>发送时间</th>
      <th>跳转目的地</th>
    </tr>
  </thead>
  <tbody>
    <% messages.each do|message| %>
      <tr>
        <td>message.type</td>
        <td>message.creator_type_cn</td>
        <td>message.notification_text</td>
        <td>message.title</td>
        <td>message.content</td>
        <td></td>
        <td></td>
        <td>message.created_at.strfcn_time</td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
